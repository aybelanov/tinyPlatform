        <div class="col-md-3">
            <app-label asp-for="PlatformInformationSettings.FacebookLink" />
        </div>
        <div class="col-md-9">
            <app-editor asp-for="PlatformInformationSettings.FacebookLink" />
            <span asp-validation-for="PlatformInformationSettings.FacebookLink"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <app-label asp-for="PlatformInformationSettings.TwitterLink" />
