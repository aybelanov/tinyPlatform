        <div class="col-md-9">
            <app-editor asp-for="PlatformInformationSettings.TwitterLink" />
            <span asp-validation-for="PlatformInformationSettings.TwitterLink"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <app-label asp-for="PlatformInformationSettings.YoutubeLink" />
        </div>
        <div class="col-md-9">
            <app-editor asp-for="PlatformInformationSettings.YoutubeLink" />
            <span asp-validation-for="PlatformInformationSettings.YoutubeLink"></span>
