{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
	  "mytemplate"
    ],
    "globalMetadata": {
      "_appName": "docs.tinyplat.com",
      "_appTitle": "docs.tinyplat.com",
      "_enableSearch": true,
	  "_disableNavbar": true,
      "pdf": false
    }
  }
}